<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sun.app_1">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".feature.activity.IntentDemoActivity"
            android:screenOrientation="fullSensor"></activity>
        <activity
            android:name=".feature.activity.StartForResultDemoActivity"
            android:screenOrientation="fullSensor"></activity>
        <activity
            android:name=".feature.activity.TransprantDemoActivity"
            android:screenOrientation="fullSensor"
            android:theme="@style/TransprantTheme"></activity>

        <activity
            android:name=".feature.activity.LifeCycleDemoActivity"
            android:configChanges="orientation"
            android:screenOrientation="fullSensor"></activity>
    </application>

</manifest>